clc; clear all; close all;
a=imread('/MATLAB Drive/All Images Files/smurfs (1).jpg'); b=rgb2gray(a); c=mat2gray(b);
d=im2bw(b); subplot(2,2,1); imshow(a); title('original image'); subplot(2,2,2); imshow(b); title('grayscale image'); subplot(2,2,3); imshow(c);
title('matrix converted image'); subplot(2,2,4);
imshow(d); title('binary image');
